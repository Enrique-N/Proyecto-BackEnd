<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">First</th>
                <th scope="col">Last</th>
                <th scope="col">Handle</th>
            </tr>
        </thead>
        <tbody>
            <% obj.forEach( item=> { %>
                <tr>
                    <th scope="row">
                        <%= item.id %>
                    </th>
                    <td>
                        <%= item.nombre %>
                    </td>
                    <td>
                        <%= item.precio %>
                    </td>
                    <td>
                        <%= item.imagen %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</div>